# MCP Server Configuration Template
# Copy this file to .env and fill in your values

# Encryption key from ProjexLight platform
MCP_ENCRYPTION_KEY=your_encryption_key_here

# Workspace path (usually /workspace in Docker)
WORKSPACE_PATH=/workspace

# MCP server port
MCP_SERVER_PORT=8766
MCP_SERVER_HOST=0.0.0.0

# Optional: Override config file path
# MCP_CONFIG_FILE=mcp-config.json
